{
  "name": "Github batch",
  "version": "1.0.1",
  "description": "Batch delete of issue in github on chrome extension",
  "manifest_version": 3,
  "author": "personalc6",
  "icons": {
    "16": "github-batch16.png",
    "32": "github-batch32.png",
    "48": "github-batch48.png",
    "128": "github-batch128.png"
  },
  "action": {
    "default_title": "Github batch"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://*.github.com/*/*/issues*"
  ]
}